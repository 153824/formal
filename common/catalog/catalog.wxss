/* common/catalog/catalog.wxss */
.catalog{
    display: flex;
    align-items: center;
    height:80rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #070922FF;
    background:rgba(7,9,34,0.03);
}
.title{
    display: flex;
    align-items: center;
    position: relative;
    height: 40rpx;
    margin-left: 40rpx;
}
.title::after{
    content: "";
    position: absolute;
    top: 50%;
    right: -30rpx;
    height: 40rpx;
    border: 0;
    border: 10rpx solid;
    border-color: black transparent transparent transparent;
}