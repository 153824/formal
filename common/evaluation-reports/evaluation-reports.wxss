page{
    background-color: rgba(247, 248, 250, 1)!important;
}
.program-wrapper{
    padding: 44rpx 44rpx 50rpx 44rpx;
    background-color: rgba(247, 248, 250, 1);
}
.program-title{
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 34rpx;
    font-weight: 600;
}
.new-adding{
    padding: 4rpx 20rpx;
    height: 42rpx;
    background: #6DD400CC;
    border-radius: 0rpx 20rpx 0rpx 20rpx;
    color: #fff;
    font-size: 24rpx;
    margin-left: 28rpx;
}
.more{
    font-size: 26rpx;
    color: #353EE8FF;
}
.program-body{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 20rpx;
    padding: 40rpx 30rpx;
    border-radius: 20rpx;
    box-shadow:0rpx 2rpx 8rpx 0rpx rgba(0,0,0,0.05);
    background-color: #fff;
}
.reply-track-body{
    padding: 30rpx 30rpx;
    height: 146rpx;
}
.evaluation-wrapper{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex-wrap: nowrap;
    flex: 1;
    margin-left: 36rpx;
}
.reply-track-info{
    font-size: 26rpx;
    color: #B8B8B8FF;
}
.count,.date{
    color: #353EE8FF;
}
.evaluation-name{
    white-space: nowrap;
    font-size: 30rpx;
    font-weight: 500;
}
.evaluation-info{
    color: #AAABB0FF;
    font-size: 26rpx;
    font-weight: 500;
}
.user-info-wrapper{
    max-width: 300rpx;
}
.user-info{
    display: flex;
    align-items: center;
    font-size: 30rpx;
    font-weight: 500;
    color: #070922FF;
}
.recommend-grade{
    padding: 5rpx 14rpx;
    font-size: 22rpx;
    border-radius: 30rpx;
    color: #353EE8FF;
    background-color: #353EE812;
    margin-left: 20rpx;
}
.send-evaluation{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #353EE8FF;
    font-size: 26rpx;
}
.the-last-report{
    position: relative;
    height: 90rpx;
    padding: 30rpx 36rpx;
    margin-top: 24rpx;
}
.new-adding-mark{
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    right: 0;
    height: 42rpx;
    border-radius:0rpx 20rpx 0rpx 20rpx;
    background-color: #6DD400CC;
    padding: 4rpx 28rpx;
    font-size: 24rpx;
    color: #fff;
}
.report-created-time{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    color: #AEAEAEFF;
    font-size: 26rpx;
}
.user-answer-evaluation{
    font-size: 26rpx;
}
.van-search-custom{
    position: fixed;
    top: 40rpx;
    left: 0;
    right: 0;
    padding: 0 44rpx;
    border-radius: 20rpx;
    background-color: rgba(247, 248, 250, 1) !important;
    z-index: 999;
}
.more-report-wrapper{
    position: absolute;
    top: 148rpx;
    width: calc(100% - 88rpx);
}
.van-search-custom-field{
    padding: 0 0 !important;
    /*padding-left: 0 !important;*/
    /*padding-right: 0 !important;*/
    background-color: #fff !important;
    border-radius: 20rpx !important;
    height: 100% !important;
}
.van-search-custom-input{
    background-color: #fff !important;
}
.van-search__content{
    padding-left: 0 !important;
}
.field-index--van-field{
    padding: unset !important;
}
.van-cell--borderless{
    padding: unset !important;
}
.no-margin-top{
    margin-top: 0 !important;
}
.recommend-score{
    padding: 5rpx 14rpx;
    font-size: 22rpx;
    border-radius: 30rpx;
    color: #353EE8FF;
    background-color: #353EE812;
    margin-left: 10rpx;
}
.search-report__wrapper{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-bottom: 40rpx;
}
.seacr-report__input{
    flex: 1;
    padding: 16rpx 16rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.05);
    border-radius: 10rpx;
}
.search-report__by-name{
    margin-right: 30rpx;
}
.seacr-report__input > input{
    text-align: center;
}
.target-evaluation__wrapper{
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.target-evaluation{
    flex: 1;
    text-align: center;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(7, 9, 34, 0.8);
    line-height: 40rpx;
}
.search-report__by-evaluation{
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
}
.search-by-evaluation-result__wrapper{
    position: absolute;
    top: 140rpx;
    left: 279rpx;
    width: 49%;
    height: 680rpx;
    padding: 30rpx 30rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 18rpx 56rpx 16rpx rgba(0, 0, 0, 0.02), 0rpx 12rpx 32rpx 0rpx rgba(0, 0, 0, 0.04), 0rpx 6rpx 12rpx -8rpx rgba(0, 0, 0, 0.06);
    border-radius: 10rpx;
    z-index: 9999;
}
.evaluation-item{
    margin-bottom: 20rpx;
    padding-bottom: 20rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #303113;
    line-height: 40rpx;
    border-bottom: 2rpx solid #E9E9E9;
}
.is-selected{
    font-size: 28rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #353EE8;
    line-height: 40rpx;
}
.van-overlay{
    background-color: #00000000!important;
}
