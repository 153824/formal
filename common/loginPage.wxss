/* common/loginPage.wxss */

.content {
  text-align: center;
  padding: 198rpx 60rpx 0;
}

.content image {
  width: 440rpx;
  height: 332rpx;
}

.content .txt {
  margin-top: 68rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFangSC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 50rpx;
}

.getUserInfo {
  margin: 80rpx 60rpx 0;
  height: 88rpx;
  background: rgba(0, 167, 98, 1);
  border-radius: 8rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 88rpx;
  letter-spacing: 1rpx;
}

.getUserInfo image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
  position: relative;
  top: 14rpx;
}
