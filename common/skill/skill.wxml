<!--common/skill/skill.wxml-->
  <view class="skill-wrapper" style="margin-left:{{ marginLeft }}rpx;margin-right:{{ marginRight }}rpx;">
    <view class="skill-set">
      <view class="skill-child" bindtap="changePage" data-id="{{ objectId }}">
        <view class="test-introduce">
          <image class="cover" src="{{ image }}" style="width: {{width}}rpx;height: {{width}}rpx;"></image>
          <view class="introduce">
            <text class="title" style="font-size: {{titleFontSize}}rpx;">{{ title }}</text>
            <text class="word" style="font-size: {{subTitleFontSize}}rpx;">{{ subtitle }}</text>
          </view>
        </view>
        <view class="test-use-info">
          <text wx:if="{{ companyUseNum > 0 }}" class="use-count">{{ companyUseNum }}家企业在用</text>
          <text wx:else class="use-count">{{ companyUseName }}在用</text>
          <free></free>
        </view>
      </view>
    </view>
  </view>
