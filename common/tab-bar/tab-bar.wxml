<view wx:if="{{ !isWxWork && !is3rd }}">
    <van-tabbar active="{{ active }}" bind:change="onChange" border="{{ true }}" active-color="#5960ED" z-index="9999">
        <van-tabbar-item id="tabbar" class="tabbar" wx:for="{{ wxPage }}" wx:key="unique">
            <image slot="icon" src="{{ item.normal }}" mode="aspectFit" style="width: 24px; height: 24px;"/>
            <image slot="icon-active" src="{{ item.active }}" mode="aspectFit" style="width: 24px; height: 24px;"/>
            {{item.text}}
        </van-tabbar-item>
    </van-tabbar>
</view>
