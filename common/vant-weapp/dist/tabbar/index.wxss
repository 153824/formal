@import '../common/index.wxss';

.van-tabbar {
    display: -webkit-flex;
    display: flex;
    box-sizing: initial;
    width: 100%;
    height: 60px;
    background-color: #fff;
    border: 1px solid rgba(7, 9, 34, 0.1);
}

.van-tabbar--fixed {
    position: fixed;
    bottom: 0;
    left: 0
}

.van-tabbar--safe {
    padding-bottom: env(safe-area-inset-bottom)
}
