/* index/mySharePapers.wxss */
.title {
  padding: 4rpx 42rpx 62rpx 34rpx;
}

.title .name {
  font-size: 60rpx;
  font-family: PingFangSC-Semibold, PingFangSC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 84rpx;
  letter-spacing: 1px;
}

.title .share {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(89, 96, 237, 1);
  line-height: 40rpx;
  letter-spacing: 1px;
  text-decoration: underline;
}

.title .changeOrder {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(92, 97, 113, 1);
  line-height: 40rpx;
}

.title .changeOrder image {
  width: 24rpx;
  height: 16rpx;
  margin-right: 18rpx;
}
.list {
  padding: 0 44rpx 40rpx;
}

.timeView {
  font-size: 3r6px;
  font-family: PingFangSC-Semibold, PingFangSC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 50rpx;
  letter-spacing: 1px;
  padding: 40rpx 0 0 20rpx;
}

.item {
  padding: 16rpx 46rpx 34rpx 26rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 4rpx 16rpx 0px rgba(207, 216, 240, 0.79);
  border-radius: 12rpx;
  margin-top: 20rpx;
}

.item image {
  width: 14rpx;
  height: 26rpx;
}

.item .name {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFangSC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 44rpx;
  letter-spacing: 1px;
  margin-bottom: 22rpx;
}

.item .num {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(89, 96, 237, 1);
  line-height: 40rpx;
}

.item .num1 {
  margin-left: 60rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(250, 140, 22, 1);
  line-height: 40rpx;
}

.listNoData {
  padding: 0;
  background-color: transparent;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
