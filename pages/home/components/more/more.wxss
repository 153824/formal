page{

}
.content-wrapper{
    position: relative;
    overflow-x: hidden;
}
.header-wrapper{
    height: 774rpx;
    padding: 0 50rpx 60rpx 50rpx;
}
.social-header-wrapper{
    position: relative;
    height: 1082rpx;
}
.title,.detail{
    font-size: 40rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
}
.detail{
    margin-top: 20rpx;
    font-size: 34rpx;
    font-weight: 400;
}
.header-bottom-wrapper{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    height: 460rpx;
    margin-top: 60rpx;
}
.orange-btn-all-radius{
    width: 100% !important;
}
.orange-btn{
    padding: unset;
    margin: unset;
    width: 590rpx;
    height: 82rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    background-color: rgba(236, 101, 64, 1);
    color: #FFFFFF;
    border-radius: 42rpx;
}
.border-radius-none-top{
    border-top-left-radius: unset;
    border-top-right-radius: unset;
}
.body-wrapper{
    padding: 40rpx 50rpx;
}
.tools-title{
    display: flex;
    justify-content: center;
    font-size: 34rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #5B5B5B;
}
.tools-item-content{
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 40rpx;
    margin-bottom: 80rpx;
}
.school-header-wrapper{
    height: 670rpx;
}
.header-bottom-content{
    width: 570rpx;
    height: 180rpx;
    padding: 50rpx 40rpx;
    background-color: #fff;
    box-shadow: 0rpx 0rpx 20rpx 12rpx rgba(90, 90, 90, 0.1);
    border-radius: 20rpx;
}
.school-header-bottom-wrapper{
    height: 400rpx;
}
.risk-content-wrapper{
    position: relative;
    padding-bottom: 0;
}
.risk-bottom-wrapper{
    height: 374rpx;
}
.brain-content-wrapper{
    position: relative;
    height: 811rpx;
}
.brain-header-bottom-wrapper{
    height: 615rpx;
}
.brain-detail-content-wrapper{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 38rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
}
.detail-block{
    display: flex;
    justify-content: space-between;
    width: 100%;
    margin-top: 40rpx;
}
.detail-block > view{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    width: 192rpx;
    height: 154rpx;
    background: #3CA1A4;
    box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.15);
    border-radius: 30rpx;
    font-size: 30rpx;
}
.risk-btn{
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 88rpx;
    border-radius: unset;
    padding: 20rpx 60rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 600;
}
.risk-btn-text{
    display: flex;
    flex-direction: column;
}
.risk-orange-btn{
    background: linear-gradient(90deg, #E96D51 0%, #E1A55C 100%);
}
.arrow-wrapper{
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    right: 0;
    width: 160rpx;
    height: 100%;
    background: rgba(255, 255, 255, 0.28);
    box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(255, 255, 255, 0.2);
    border-radius: 64rpx 0rpx 0rpx 64rpx;
}
.header-social-bottom-wrapper{
    height: auto;
}
.ques{
    font-size: 34rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
    margin: 30rpx 0 50rpx 0;
}
.orange-arrow-btn{
    background-color: rgba(255, 243, 0, 0.4);
}
.tools-title-text{
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.school-tools-title::before,.school-tools-title::after{
    border-color: rgba(53, 156, 170, 1) !important;
}

.risk-tools-title::before,.risk-tools-title::after{
    border-color: rgba(244, 171, 51, 1) !important;
}

.brain-tools-title::before,.brain-tools-title::after{
    border-color: rgba(89, 155, 224, 0.5) !important;
}

.social-tools-title::before,.social-tools-title::after{
    border-color: rgba(245, 90, 152, 1) !important;
}
.footer{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 15rpx;
    height: 104rpx;
    margin-top: 150rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #787777;
}
.footer > image {
    margin-top: 16rpx;
}
.tools-title-text::before{
    margin-right: 10rpx;
    content: "";
    width: 0;
    height: 0;
    border-bottom: 30rpx solid red;
    border-right: 12rpx solid transparent !important;
}
.tools-title-text::after{
    margin-left: 10rpx;
    content: "";
    width: 0;
    height: 0;
    border-top: 30rpx solid red;
    border-left: 12rpx solid transparent !important;
}
.tools-item-area{
 position: relative;
}
.risk-tools-location{
    position: relative;
}
.risk-tools-location-btn{
    position: absolute;
    bottom: -45rpx;
}
.risk-bg-wrapper,.brain-bg-wrapper,.social-bg-wrapper{
    position: absolute;
    top: 130rpx;
    right: 0;
}
.brain-bg-wrapper,.social-bg-wrapper{
    right: 75rpx;
}
.brain-bg,.social-bg{
    position: absolute;
}
.circle-normal{
    position: absolute;
    width: 168rpx;
    height: 168rpx;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.06);
}
.circle-small{
    top: -20px;
    right: -230rpx;
    z-index: 99;
}
.circle-medium{
    top: -28rpx;
    right: -190rpx;
}
.across{
    position: relative;
}
.flash-across {
    position:absolute;
    animation:changeImg 3s ease 0s;
    top:0;
    width:30%;
    height:100%;
    content:"";
    background:linear-gradient(to right,rgba(255,255,255,0) 0,rgba(255,255,255,.4) 50%,rgba(255,255,255,0) 100%);
    transform:skewX(-45deg);
    animation-iteration-count:infinite;
}
.liner-orange{
    background: linear-gradient(90deg, #E35E4C 0%, #F48D5B 100%);
}
.liner-green{
    background: linear-gradient(90deg, #128BA0 0%, #94CBC3 100%);
}
.liner-blue{
    background: linear-gradient(90deg, #1E67D1 0%, #5C9EE1 100%);
}
.liner-pink{
    background: linear-gradient(90deg, #FD4351 0%, #F55A98 100%);
}
@keyframes changeImg {
    from {
        left:-100%;
    }
    100% {
        left:150%;
    }
}
