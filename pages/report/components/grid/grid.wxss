.hola-grid{
    width: 650rpx;
    height: 660rpx;
    padding: 60rpx 84rpx 80rpx 84rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(200, 200, 200, 0.34);
    border-radius: 20rpx;
}
.hola-grid-axis{
    position: relative;
    width: 100%;
    height: 100%;
    border-left: 4rpx solid rgba(53, 62, 232, 1);
    border-bottom: 4rpx solid rgba(53, 62, 232, 1);
}
.axis-x-text,.axis-y-text{
    position: absolute;
    text-align: center;
    font-size: 30rpx;
    font-weight: 500;
    color: rgba(53, 62, 232, 1);
    background-color: #fff;
    display: flex;
    flex-wrap: nowrap;
}
.axis-x-text{
    bottom: -20rpx;
    left: 50%;
    transform: translateX(-50%);
}
.axis-y-text{
    left: -25rpx;
    top: 50%;
    transform: translateY(-50%);
    display: flex;
    justify-content: center;
    width: 40rpx;
    height: auto;
    writing-mode: vertical-lr;
}
.axis-data{
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    width: 100%;
    height: 100%;
    padding: 0 0 20rpx 80rpx;
}
.axis-data-x-2{
    display: flex;
}
.axis-data-x-2 .reach-the-standard{
    width: 40rpx;
    height: 104rpx;
    background-color: rgba(53, 62, 232, 1);
}
.data-recommend{
    width: 200rpx;
    height: 200rpx;
}
.van-grid-custom{
    margin-bottom: -25rpx;
    padding-left: 35rpx;
    transform: rotate(-90deg);
    transform-origin: center center;
}
.van-grid-icon-custom{
    display: none !important;
}
.van-grid-content-custom-first-command{
    background-color: rgba(53, 62, 232, 0.1) !important;
}
.van-grid-content-custom{
    transform-origin: center center;
    transform: rotate(90deg);
    background-color: rgba(251, 251, 251, 1);
    border-radius: 10rpx;
}
.van-grid-text-custom{
    margin-top: 0 !important;
    font-size: 30rpx !important;
    font-weight: 400 !important;
    color: rgba(212, 212, 212, 1);
}
.van-grid-text-custom-nine-cell{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 24rpx !important;
    font-weight: 400 !important;
    color: rgba(212, 212, 212, 1) !important;
}
.van-grid-text-custom-first-command{
    color: rgba(53, 62, 232, 1) !important;
}
.axis-data-x{
    display: flex;
    justify-content: space-around;
    height: 40rpx;
    font-size: 24rpx;
    transform-origin: left top;
    transform: rotate(-90deg) translateY(-55rpx) translateX(-35rpx);
}
.axis-data-x .reach-the-standard{
    padding: 2rpx 30rpx;
    color: #fff;
    border-radius: 20rpx;
    background-color: rgba(53, 62, 232, 1);
}
.axis-data-x .not-reach-the-standard{
    padding: 2rpx 30rpx;
    color: rgba(200, 200, 200, 1);
    background-color: rgba(0, 0, 0, 0);
    border-radius: 20rpx;
}
.axis-data-y{
    display: flex;
    justify-content: space-around;
    height: 40rpx;
    font-size: 24rpx;
    transform-origin: left top;
}
.axis-data-y .reach-the-standard{
    padding: 2rpx 30rpx;
    color: #fff;
    border-radius: 20rpx;
    background-color: rgba(53, 62, 232, 1);
}
.axis-data-y .not-reach-the-standard{
    padding: 2rpx 30rpx;
    color: rgba(200, 200, 200, 1);
    background-color: rgba(0, 0, 0, 0);
    border-radius: 20rpx;
}
.axis-row{
    position: absolute;
    width: 0;
    height: 0rpx;
    border: 10rpx solid transparent;
}
.axis-x-row{
    right: -28rpx;
    bottom: -11rpx;
    border-left: 20rpx solid rgba(53, 62, 232, 1);
}
.axis-y-row{
    top: -28rpx;
    left: -11rpx;
    border-bottom: 20rpx solid rgba(53, 62, 232, 1);
}
.van-grid-text-custom{
    width: 135%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: center;
}
