/*pay*/
.paypage {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex-direction: column;
  z-index: 999999999;
}

.closebox {
  width: 100%;
  height: calc(100vh - 834rpx);
}

.paybox {
  width: 100%;
  height: 946rpx;
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  overflow: hidden;
}

.paypagehead {
  padding: 40rpx 32rpx;
  height: 178rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}

.payheadleft>view {
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 44rpx;
}

.payheadleft>text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(144, 145, 156, 1);
  line-height: 40rpx;
}

.payheadright {
  width: 98rpx;
  height: 98rpx;
  background-color: #5960ed;
}

.payheadright>image {
  width: 100%;
  height: 100%;
}

.paypagebody {
  height: 128rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 32rpx 40rpx;
  box-sizing: border-box;
}

.paybodyleft {
  font-size: 36rpx;
  line-height: 50rpx;
  color: #fa541c;
  font-weight: 600;
}

.paybodyright {
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.paybodyright button, .paybodyright input {
  width: 96rpx;
  height: 88rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 0 6rpx 6rpx 0;
  text-align: center;
  line-height: 88rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #999;
}

.paybodyright input {
  margin: 0 2rpx;
  color: #333440;
  font-size: 32rpx;
  font-weight: 500;
}

.paybodyright button::after {
  border: none;
}

.wechatpay, .ticketpay {
  /* margin-top: 14rpx; */
  margin-bottom: 4rpx;
  /* height: 194rpx; */
  background: rgba(255, 255, 255, 1);
  padding: 40rpx 32rpx;
  box-sizing: border-box;
}

.ticketpay {
  border-top: 1px solid #e7e7e7;
}

.wechatpayimg {
  width: 40rpx;
  height: 36rpx;
  margin-right: 26rpx;
}

.checkicon {
  width: 36rpx;
  height: 36rpx;
  margin-left: 40rpx;
}

.ticketpayimg {
  width: 40rpx;
  height: 40rpx;
  margin-right: 26rpx;
}

.wechatpaybox {
  padding-top: 40rpx;
}

.wechatpaybox, .ticketpaybox {
  /* margin-top: 40rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wechatpaybox>view, .ticketpaybox>view {
  display: flex;
  align-items: center;
}

.allpaytext {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
  line-height: 40rpx;
}

.allpaygraytext {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(144, 145, 156, 1);
  line-height: 40rpx;
}

