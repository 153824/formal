/* station/report.wxss */
.header{
  display: flex;
  min-height: 250rpx;
  flex-wrap: wrap;
}
.header-wrapper{
  display: flex;
  flex-wrap: wrap;
  padding: 34rpx 64rpx 40rpx 40rpx;
  background-color: #fff;
  width: 100%;
}
.cover-wrapper{
  width: 140rpx;
  height: 140rpx;
}
.cover-wrapper > image{
  width: 140rpx;
  height: 140rpx;
}
.info-wrapper{
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  /*align-items: center;*/
}
.info-wrapper > .title{
  display: flex;
  font-size: 44rpx;
  font-weight: 500;
  color: #070922FF;
}
.price-wrapper{
  margin-top: 22rpx;
  margin-bottom: 20rpx;
}
.price-wrapper  .old{
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  color: rgba(7, 9, 34, 0.4);
  font-size: 24rpx;
  text-decoration: line-through;
}
.price-wrapper  .new{
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  margin-left: 4rpx;
  font-size: 32rpx;
  font-weight:500;
  color: rgba(184,112,43,1);
}
.use-time{
  color: rgba(7, 9, 34, 0.6);
  font-size: 24rpx;
}
.leave-wrapper{
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-top: 36rpx;
  align-items: center;
  padding: 20rpx 40rpx;
  height: 40rpx;
  border-radius: 20rpx;
  color: #070922FF;
  background:rgba(53,62,232,0.03);
  font-size: 28rpx;
  border: 1px solid rgba(53,62,232,0.2);
}
.leave-wrapper > text{
  display: inline-block;
  line-height: 40rpx;
}
.buy-more{
  font-weight:500;
  color: #353EE8FF;
}
.button-group{
  display: flex;
  flex-direction: column;
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  z-index: 999;
}
.button-group-child{
  display: flex;
}
.meal{
  width: 100%;
  height: 100%;
}
.gotoguide-btn{

}
