<view wx:if="{{ !isWxWork && false }}">
    <view class="user-page-wrapper">
        <view class="head">
            <view class="user-personal-info">
                <block wx:if="{{userInfo.isBind&&false}}">
                    <image src="{{userInfo.avatar || userInfo.avatarUrl || '../report/images/report@icon-avatar.png'}}"
                           style="margin-bottom: 32rpx;width: 128rpx;height: 128rpx;border-radius: 50%;"></image>
                    <text></text>
                </block>
                <block wx:else>
                    <button class="user-info-button" open-type="getUserInfo"
                            bindgetuserinfo="getUserInfo">
                        <image src="{{userInfo.avatar || userInfo.avatarUrl || '../report/images/report@icon-avatar.png'}}"
                               style="margin-bottom: 32rpx;width: 128rpx;height: 128rpx;border-radius: 50%;"></image>
                        <text>{{userInfo.nickname || userInfo.nickName || userInfo.info.nickName}}</text>
                    </button>
                </block>
            </view>
        </view>
        <view class="content">
            <scroll-view class="content-scroll-area" scroll-y>
                <view class="user-action-item" bindtap="goToReceiveReports">
                    <view class="title">
                        <image src="./images/user@othors-invite-to-read-report.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>他人分享给我的报告</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
                <view class="user-action-item" bindtap="goToReceiveEvaluation">
                    <view class="title">
                        <image src="./images/user@othors-invite-to-reply-report.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>他人邀请我参加的测评</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
                <view class="user-action-item" bindtap="goToAddGroup">
                    <view class="title">
                        <image src="./images/user@connect-serving.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>成为共创用户</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
            </scroll-view>
        </view>
    </view>
    <hola-tab-bar active="{{ 3 }}"></hola-tab-bar>
</view>
<view wx:elif="{{ isWxWork && isWxWorkAdmin && false }}">
    <view class="user-page-wrapper">
        <view class="head">
            <view class="user-personal-info">
                <image src="{{userInfo.avatar || userInfo.avatarUrl || '../report/images/report@icon-avatar.png'}}"
                       style="margin-bottom: 32rpx;width: 128rpx;height: 128rpx;border-radius: 50%;background-color: black;"></image>
                <text>{{userInfo.name}}</text>
            </view>
        </view>
        <view class="content">
            <scroll-view class="content-scroll-area" scroll-y>
                <view class="user-action-item" bindtap="goToReceiveReports">
                    <view class="title">
                        <image src="./images/user@othors-invite-to-read-report.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>他人分享给我的报告</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
                <view class="user-action-item" bindtap="goToReceiveEvaluation">
                    <view class="title">
                        <image src="./images/user@othors-invite-to-reply-report.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>他人邀请我参加的测评</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
                <view class="user-action-item" bindtap="goToServingClient">
                    <view class="title">
                        <image src="./images/user@connect-serving.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>联系专属客服</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
            </scroll-view>
        </view>
    </view>
    <hola-tab-bar active="{{ 1 }}"></hola-tab-bar>
</view>
<view wx:elif="{{ true }}">
    <view class="user-page-wrapper">
        <view class="head">
            <view class="user-personal-info">
                <image src="{{userInfo.avatar || userInfo.avatarUrl || '../report/images/report@icon-avatar.png'}}"
                       style="margin-bottom: 32rpx;width: 128rpx;height: 128rpx;border-radius: 50%;background-color: black;"></image>
                <text>{{userInfo.userCompany.name || userInfo.company.name}}的员工</text>
            </view>
        </view>
        <view class="content">
            <scroll-view class="content-scroll-area" scroll-y>
                <view class="user-action-item" bindtap="goToReceiveReports">
                    <view class="title">
                        <image src="./images/user@othors-invite-to-read-report.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>他人分享给我的报告</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
                <view class="user-action-item" style="position: relative;">
                    <hola-serving  type="rectangle"></hola-serving>
                    <view class="title" style="position: absolute;">
                        <image src="./images/user@connect-serving.png"
                               style="width: 44rpx;height: 44rpx;margin-right: 20rpx;"></image>
                        <text>联系专属客服</text>
                    </view>
                    <image src="../../images/icon/icon@arrow-left.png" style="width: 12rpx;height: 20rpx;"></image>
                </view>
            </scroll-view>
        </view>
    </view>
    <hola-tab-bar active="{{ 1 }}"></hola-tab-bar>
</view>
