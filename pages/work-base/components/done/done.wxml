<!--test/finish.wxml-->
<hola-title type="navigator" url="{{targetURL}}"/>
<view class="fullPage" style="{{isMp?'background:#fff;':''}}">
	<view wx:if="{{isTest}}" class="pageView">
		<image src="../../images/done@icon-done.png"></image>
		<view class="mindMsg">模拟作答已结束</view>
		<view class="mindMsg">感谢您的参与</view>
		<view class="reportBtn" bindtap="toBack">返回</view>
	</view>
	<view class="pageView" wx:elif="{{!isMp}}">
		<image src="../../images/done@icon-done.png"></image>
		<view class="mindMsg">您的作答结果已提交</view>
		<view class="mindMsg">感谢您的参与</view>
		<block wx:if="{{ reportPermit == 'APPROVED' }}">
			<button style="width:260px;" class="reportBtn" bindtap="toDetail">查看报告</button>
		</block>
		<view class="reportBtn" bindtap="toApply" wx:if="{{ reportPermit == 'DISABLED' }}">向HR申请查看报告</view>
	</view>
	<view class="mpView" wx:else>
		<image src="../../images/done@icon-checked.png"></image>
		<view class="mindMsg">申请成功</view>
		<view class="mindMsg">HR同意后，系统将通过小程序消息通知您</view>
	</view>
</view>
