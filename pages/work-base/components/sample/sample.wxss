  height: 100%;
  background: #F7F8FA;
  padding-top: 1rpx;
}
.sample-explain,.sample-examplesQue{
  background: #FEFFFE;
  box-shadow: 0px 2px 4px 0px #D8DBEB;
  border-radius: 20rpx;
  margin: 40rpx;
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.sample-explainTitle,.sample-examplesQueTitle{
  position: relative;
  font-size: 44rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #2F3038;
  line-height: 60rpx;
  margin-bottom: 30rpx;
}
.sample-explainContent{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #303113;
  line-height: 50rpx;
}
.sample-examplesQueContent{
  display: flex;
  flex-direction: column;
}
.sample-examplesQueContentTitle{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #303113;
  line-height: 50rpx;
}
/*拖拽*/
.drag-item-wrap {
  background: #f7f7f7; }
.sample-examplesQueContentAnalysis{
  padding: 30rpx;
  background: #5AD8A6;
  border-radius: 10px;
  color: white;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
}
.sample-examplesQueContentAnalysis:not(:last-child){
  margin-bottom: 90rpx;
}
.isChangeQue { 
  position: fixed;
  z-index: 999999;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: transparent;
}
.imgTimeout{
  font-size: 30rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #353EE8;
  line-height: 42rpx;
}
.imgTimeoutContent{
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
}
.imgQue{
  display: flex;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  line-height: 42rpx;
  font-weight: 600;
  margin-bottom: 30rpx;
}
.imgQue-title{
  color: #353EE8;
}
.titleImgButton{
  display: block;
  padding: 12rpx 0;
  background: #353EE8;
  text-align: center;
  width: 210rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 44rpx;
  cursor: pointer;
  border-radius: 10rpx;
  transition: all 1s;
}
.imgContent{
  margin-bottom: 40rpx;
}
.showImg{
  padding: 0 40rpx;
}
.showImg .question-stem-rich-text-img{
  max-width: 100%;
}
.sample-toAnswer{
  height: 90rpx;
  background: #353EE8;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.3);
  border-radius: 10rpx;
  text-align: center;
  line-height: 90rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
  margin: 0 40rpx;
  margin-bottom: 120rpx;
}
.sample-toAnswerWrap{
  padding-bottom: 50rpx;
}
.sample-examplesQueContentOption{
  padding: 30rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #303113;
  line-height: 50rpx;
  background: #FAFAFA;
  border-radius: 10px;
  margin-top: 30rpx;
}
.sample-explainTitle::after,.sample-examplesQueTitle::after{
  content: '';
  position: absolute;
  bottom: -10rpx;
  left: 0;
  width: 34px;
  height: 4px;
  background: #353EE8;
  border-radius: 2px;
}