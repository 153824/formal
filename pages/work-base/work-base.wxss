.content-scroll-area{
}

.program-wrapper{
    padding: 22rpx 44rpx 22rpx 44rpx;
    z-index: 99999999;
}
.program-title{
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 34rpx;
    font-weight: 600;
}
.new-adding{
    padding: 4rpx 20rpx;
    height: 42rpx;
    background: #6DD400CC;
    border-radius: 0rpx 20rpx 0rpx 20rpx;
    color: #fff;
    font-size: 24rpx;
