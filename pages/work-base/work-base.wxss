.reply-track-body{
    padding: 30rpx 36rpx;
    height: 146rpx;
}
.reply-track-body:last-child{
    margin-bottom: 44rpx;
}
.evaluation-wrapper{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex-wrap: nowrap;
    flex: 1;
    margin-left: 36rpx;
}
.track-info-wrapper{
    margin-left: 0rpx;
}
.reply-track-info{
    font-size: 26rpx;
    color: #B8B8B8FF;
}
.count,.date{
    color: #353EE8FF;
}
.evaluation-name{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 30rpx;
    font-weight: 500;
}
.evaluation-info{
    color: #AAABB0FF;
    font-size: 26rpx;
    font-weight: 500;
}
.user-info-wrapper{
    max-width: 300rpx;
}
.user-info{
    display: flex;
    align-items: center;
    font-size: 30rpx;
    font-weight: 500;
    color: #070922FF;
}
.recommend-grade{
    margin-left: 20rpx;
    padding: 5rpx 14rpx;
    font-size: 22rpx;
    border-radius: 30rpx;
    color: #353EE8FF;
    background-color: #353EE812;
}
.recommend-score{
    padding: 5rpx 14rpx;
    font-size: 22rpx;
    border-radius: 30rpx;
    color: #353EE8FF;
    background-color: #353EE812;
    margin-left: 10rpx;
}
.send-evaluation{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #353EE8FF;
    font-size: 26rpx;
}
.the-last-report{
    position: relative;
    height: 90rpx;
    padding: 30rpx 36rpx;
    margin-top: 24rpx;
}
.new-adding-mark{
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    right: 0;
    height: 42rpx;
    border-radius:0rpx 20rpx 0rpx 20rpx;
    background-color: #6DD400CC;
    padding: 4rpx 28rpx;
    font-size: 24rpx;
    color: #fff;
}
.report-created-time{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    color: #AEAEAEFF;
    font-size: 26rpx;
}
.user-answer-evaluation{
    font-size: 26rpx;

}
/*首页骨架屏*/
.skeleton-index-page-wrapper{
    overflow: hidden !important;
}
.skeleton-banner-wrapper{
    height: 510rpx;
    padding: 40rpx 40rpx;
    background-color: #fff;
    box-shadow:0px 2px 8px 0px rgba(53,62,232,0.1);
    border-radius:0px 0px 40px 40px;
}
.skeleton-banner-wrapper .van-skeleton{
    padding: unset !important;
}
.skeleton-banner .van-skeleton__row,.van-skeleton__title{
    height: 320rpx !important;
    border-radius: 20rpx !important;
}
.skeleton-functional{
    display: flex;
    justify-content: space-between;
    margin-top: 42rpx;
    padding: 0 52rpx;
}
.skeleton-functional-child{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.skeleton-functional-child .van-skeleton__row,.van-skeleton__title{
    width: 88rpx !important;
    height: 88rpx !important;
    border-radius: 50%;
}
.skeleton-functional-child-rectangle-item .van-skeleton__row,.van-skeleton__title{
    width: 112rpx !important;
    height: 40rpx !important;
    border-radius: 6rpx !important;
}

.skeleton-content-wrapper{
    padding: 0 40rpx;
}
.skeleton-content-child{
    margin-top: 50rpx;
}
.skeleton-column-title{

}
.skeleton-column-title .van-skeleton__row,.van-skeleton__title{
    width: 240rpx !important;
    height: 52rpx !important;
    border-radius: 4px;
}
.skeleton-underline .van-skeleton__row,.van-skeleton__title{
    margin-top: 8rpx;
    width: 80rpx !important;
    height: 12rpx !important;
}
.skeleton-column-child-wrapper{
    display: flex;
    margin-top: 40rpx;
}
.skeleton-column-child-wrapper > .skeleton-column-child:first-child{
    margin-right: 40rpx;
}
.skeleton-column-banner .van-skeleton__row,.van-skeleton__title{
    width: 352rpx !important;
    height: 160rpx !important;
    border-radius: 8rpx;
}
.skeleton-column-child-title{
    margin-top: 20rpx;
}
.skeleton-column-child-title .van-skeleton__row,.van-skeleton__title{
    width: 220rpx !important;
    height: 44rpx !important;
    border-radius: 4rpx;
}
.skeleton-column-child-info{
    margin-top: 12rpx;
}
.skeleton-column-child-info .van-skeleton__row,.van-skeleton__title{
    width: 352rpx !important;
    height: 16rpx !important;
    border-radius: 4rpx;
}
.skeleton-column-child-info .van-skeleton, .skeleton-column-child-title .van-skeleton, .skeleton-column-banner .van-skeleton, .skeleton-column-title .van-skeleton, .skeleton-underline .van-skeleton{
    padding: unset !important;
}
.skeleton-column-child-info .van-skeleton__row,.van-skeleton__title{
    margin-top: 12rpx !important;
}

