.listtext {
  justify-content: space-between;
  align-items: flex-start;
  flex-direction: column;
}

.bigtitle {
  font-size: 42rpx;
  display: block;
  line-height: 44rpx;
  color: #323541;
  font-weight: 600;
  margin-bottom: 14rpx;
  letter-spacing: 1px;
}

.bigtitle1 {
  font-size: 32rpx;
  display: block;
  line-height: 44rpx;
  color: #323541;
  font-weight: 400;
  margin-bottom: 14rpx;
  letter-spacing: 1px;
}

.blueline {
  width: 12rpx;
  height: 36rpx;
  background-color: #5960ed;
  border-radius: 6rpx;
  display: inline-block;
  margin-right: 16rpx;
}

.detail {
  width: 100%;
  display: block;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #323541;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  /* letter-spacing: 1px; */
  word-wrap: break-word;
  word-break: break-all;
  white-space: pre-line;
}

.footerlist {
  width: 100%;
  height: 34rpx;
  line-height: 34rpx;
  display: flex;
  justify-content: space-between;
  /* align-content: flex-end; *//* margin-top: 20rpx; */
}

.flistleft>text {
  font-size: 24rpx;
  letter-spacing: 1px;
  /* line-height: 34rpx; */
  color: #90919c;
}

.flistleft>text:nth-of-type(1) {
  margin-right: 14rpx;
}

.del {
  text-align: center;
  font-size: 24rpx;
  /* letter-spacing: 1px; */
  line-height: 34rpx;
  color: #90919c;
  text-decoration: line-through;
  margin-top: 8rpx;
}

.dollar {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFangSC;
  font-weight: 500;
  color: rgba(89, 96, 237, 1);
  width: 138rpx;
  height: 54rpx;
  background: rgba(246, 246, 246, 1);
  border-radius: 27rpx;
  text-align: center;
  line-height: 54rpx;
}

.freeTest {
  text-align: center;
  width: 150rpx;
  height: 54rpx;
  background: rgba(232, 106, 18, 1);
  border-radius: 27rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(254, 255, 254, 1);
  line-height: 54rpx;
}