/* index/store/unfold.wxss */

.container {
  background-color: #f5f9fa;
  min-height: 100vh;
}

.isiphoneX {
  padding-bottom: 68rpx !important;
}

.header {
  position: relative;
  padding: 32rpx 40rpx 0rpx;
  background-color: #fff;
  box-shadow: 0px 4rpx 16rpx 0px rgba(204, 213, 237, 0.41);
  z-index: 9;
}

.bigtitle {
  font-size: 42rpx;
  letter-spacing: 1px;
  line-height: 58rpx;
  color: #323541;
}

.headertext {
  font-size: 24rpx;
  letter-spacing: 1px;
  line-height: 42rpx;
  margin-top: 20rpx;
  padding-bottom: 40rpx;
  color: #5c6171;
  /* text-indent: 48rpx; */
}

.tabs .item {
  font-size: 28rpx;
  font-weight: 500;
  letter-spacing: 1px;
  line-height: 80rpx;
  width: 180rpx;
  text-align: center;
  box-sizing: border-box;
  position: relative;
}

.tabs .item.active {
  color: #5960ed;
  box-sizing: border-box;
}

.underline {
  position: absolute;
  bottom: 0;
  width: 180rpx;
  height: 4rpx;
  left: 50%;
  transform: translateX(-50%);
  background-color: #5960ed;
}

/* list */

.bodybox {
  box-sizing: border-box;
}

.bodybox>scroll-view {
  padding-top: 22rpx;
  background-color: #fff;
  box-sizing: border-box;
}

.list {
  justify-content: space-between;
  align-items: center;
  /* margin: 20rpx auto 0; */
  padding: 18rpx 36rpx;
  /* background-color: #fff; */
  box-sizing: border-box;
  /* box-shadow: 0 4rpx 16rpx 0 rgba(204, 213, 237, 0.41); *//* border-radius: 12rpx; */
  text-align: left;
}

.list::after {
  border: none;
}

.orgbox {
  width: 140rpx;
  height: 140rpx;
  margin-right: 26rpx;
  position: relative;
}

.orgbox  image {
  width: 140rpx;
  height: 140rpx;
  margin-right: 30rpx;
  position: relative;
  border-radius: 16rpx;
}

.cover {
  width: 100%;
  height: 54rpx;
  background-color: rgba(235, 47, 47, 0.85);
  position: absolute;
  bottom: 0;
  text-align: center;
  color: #fff;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  line-height: 54rpx;
  font-size: 26rpx;
}

