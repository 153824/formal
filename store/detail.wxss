.toBuyVip .animationView {
  position: absolute;
  display: block;
  width: 84rpx;
  height: 300%;
  background-color: rgba(255, 255, 255, 0.45);
  z-index: 2;
  transform: rotate(25deg);
  top: -100%;
  -webkit-animation: animationView 1.5s linear 0.5s infinite;
  -ms-animation: animationView 1.5s linear 0.5s infinite;
  -o-animation: animationView 1.5s linear 0.5s infinite;
  animation: animationView 1.5s linear 0.5s infinite;
}

@-webkit-keyframes animationView {
  from {
    left: -170rpx;
  }

  to {
    left: 150%;
  }
}

