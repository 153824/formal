/* index/store/store.wxss */

/* 测评商城 */

.getGift {
  display: block;
  height: 80rpx;
  width: 100%;
}

.giftDlg {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.69);
  z-index: 99999;
}

.giftDlg .dlgContent {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.giftDlg .dlgContent .closeImg {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: -24rpx;
  right: -24rpx;
}

.giftDlg .dlgContent .msgImg {
  width: 494rpx;
  height: 646rpx;
  display: block;
  margin: auto;
}

.giftDlg .dlgContent .btnImg {
  width: 362rpx;
  height: 110rpx;
  display: block;
  margin: -1px auto 0;
}

.giftDlg .dlgContent .btnImg.btnImgBtn {
  background: transparent;
  position: absolute;
  opacity: 0;
  z-index: 999;
  left: 0;
  right: 0;
  width: auto;
}

.topCouponGetView {
  position: relative;
}

.topCouponGetView button {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1;
  opacity: 0;
}

.topCouponGetView image {
  display: block;
  width: 100%;
  height: 80rpx;
}

.topTestView {
  height: 80rpx;
  line-height: 80rpx;
  color: #5960ed;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  padding: 0 34rpx 0 26rpx;
  background: rgba(239, 240, 253, 1);
}

.topTestView span {
  width: 120rpx;
  line-height: 1;
  white-space: nowrap;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
}

.topTestView .icon25 {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.topTestView .icon24 {
  width: 40rpx;
  height: 40rpx;
}
.container {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.freeTest {
  text-align: center;
  width: 150rpx;
  height: 54rpx;
  background: rgba(232, 106, 18, 1);
  border-radius: 27rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(254, 255, 254, 1);
  line-height: 54rpx;
}

.banner {
  width: 100%;
  height: 276rpx;
  background-color: #5960ed;
  position: relative;
  z-index: 1;
}

.banner swiper {
  height: 260rpx;
}

.banner .slide-image {
  width: 100%;
  height: 276rpx;
}

/* card */

.box {
  position: relative;
  z-index: 2;
  background-color: #efeff3;
  margin-top: -24rpx;
  border-top-left-radius: 24rpx;
  border-top-right-radius: 24rpx;
}

/* .box>.card:nth-of-type(1) {
  border-top-left-radius: 24rpx;
  border-top-right-radius: 24rpx;
} */

.card {
  position: relative;
  background-color: #fff;
  margin-bottom: 20rpx;
  padding-bottom: 40rpx;
}

.cardhead {
  padding: 36rpx 36rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* border-bottom: 1px solid #e7e7e7; */
}

.headleft {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFangSC;
  font-weight: 600;
  color: rgba(47, 48, 56, 1);
  line-height: 44rpx;
}

.headleft>.showMore {
  font-size: 32rpx;
  letter-spacing: 1px;
  color: #2f3038;
  font-weight: 600;
}

.blueline {
  width: 12rpx;
  height: 36rpx;
  background-color: #5960ed;
  border-radius: 6rpx;
  display: inline-block;
  margin-right: 16rpx;
}

.cardhead>.showMore {
  display: inline-block;
  font-size: 28rpx;
  color: #5c6171;
}

.cardhead>.showMore>image {
  width: 10rpx;
  height: 12rpx;
  margin-left: 12rpx;
  position: relative;
  top: -4rpx;
}

.list {
  padding: 36rpx 36rpx 0rpx;
  margin: 0;
  box-sizing: border-box;
  background: none;
  text-align: left;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.list:nth-of-type(1) {
  padding-top: 25rpx;
}

.list:last-of-type {
  padding-bottom: 36rpx;
}

button::after {
  border: none;
}

input {
  outline: none;
  border: none;
  list-style: none;
}

.list::after {
  border: none;
}

.listgetphoneNumber {
  background: none;
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

.listgetphoneNumber::after {
  border: none;
}

.orgbox {
  width: 140rpx;
  height: 140rpx;
  border-radius: 16rpx;
  margin-right: 26rpx;
  position: relative;
}

.orgbox>image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 16rpx;
}

.cover {
  width: 100%;
  height: 54rpx;
  background-color: rgba(235, 47, 47, 0.85);
  position: absolute;
  bottom: 0;
  text-align: center;
  color: #fff;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  line-height: 54rpx;
  font-size: 26rpx;
}

.listtext {
  /* width: 454rpx; */
}

.bigtitle {
  font-size: 32rpx;
  display: block;
  line-height: 44rpx;
  color: #313645;
  font-weight: 400;
  margin-bottom: 14rpx;
  letter-spacing: 1px;
}

.detail {
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5c6171;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  letter-spacing: 1px;
}

.footerlist {
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* margin-top: 20rpx; */
}

.flistleft>text {
  font-size: 24rpx;
  letter-spacing: 1px;
  line-height: 34rpx;
  color: #90919c;
}

.flistleft>text:nth-of-type(1) {
  margin-right: 14rpx;
}

.del {
  text-align: center;
  font-size: 24rpx;
  /* letter-spacing: 1px; */
  line-height: 34rpx;
  color: #90919c;
  text-decoration: line-through;
  margin-top: 8rpx;
}

.dollar {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFangSC;
  font-weight: 500;
  color: rgba(89, 96, 237, 1);
  width: 138rpx;
  height: 54rpx;
  background: rgba(246, 246, 246, 1);
  border-radius: 27rpx;
  text-align: center;
  line-height: 54rpx;
}

.coverMindView {
  position: absolute;
  top: 76rpx;
  left: 68rpx;
  z-index: 9;
}

.coverMindView .bgImg {
  width: 665rpx;
  height: 168rpx;
}

.coverMindView  .closeImg {
  position: absolute;
  width: 20rpx;
  height: 24rpx;
  top: 56rpx;
  right: 36rpx;
}

.coverMindView .textView {
  position: absolute;
  top: 46rpx;
  left: 28rpx;
}

.coverMindView .textView .tt {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(254, 255, 254, 1);
  line-height: 44rpx;
}

.coverMindView .textView .tm {
  margin-top: 14rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(254, 255, 254, 1);
  line-height: 40rpx;
}

.listItem {
  margin: 40rpx 40rpx 0;
  padding: 24rpx 40rpx 26rpx 48rpx;
  background: rgba(254, 255, 254, 1);
  box-shadow: 0px 0px 18rpx 0px rgba(212, 212, 212, 0.65);
  border-radius: 4rpx;
}

.listItem .name {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 44rpx;
}

.listItem .mind {
  margin-top: 12rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(92, 97, 113, 1);
  line-height: 34rpx;
}

.listItem image {
  width: 18rpx;
  height: 24rpx;
}
