/* takephoto.wxss */
@import "../common/colorui/main.wxss";
@import "../common/colorui/icon.wxss";
.isChangeQue {
  position: fixed;
  z-index: 999999;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: transparent;
}

.container1 {
  padding: 50rpx 60rpx 0;
}

.container1 .bigtitle {
  font-size: 48rpx;
  letter-spacing: 2px;
  line-height: 66rpx;
  color: #323541;
  font-weight: 500;
}

.container1 .smalltitle {
  font-size: 24rpx;
  letter-spacing: 1px;
  line-height: 34rpx;
  color: #323541;
  margin-bottom: 60rpx;
  display: block;
}

.container1 form {
  position: relative;
}

.container1 .inputtext {
  width: 80rpx;
  position: absolute;
  top: 18rpx;
  left: 0;
}

.container1 input {
  border-bottom: 1px solid #e7e7e7;
  padding: 14rpx 0;
  text-align: right;
}

.container1 .picker {
  margin-top: 40rpx;
  padding: 14rpx 0;
  border-bottom: 1px solid #e7e7e7;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.container1 .photobox {
  margin-top: 74rpx;
  width: 632rpx;
  height: 370rpx;
  border-radius: 10rpx;
  border: 1px dashed rgba(231, 231, 231, 1);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.container1 .photobtn {
  width: 126rpx;
  height: 126rpx;
}

.container1 .photobox>view {
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 600;
  color: #323541;
  margin: 20rpx 0 10rpx;
}

.container1 .photobox>text {
  text-align: center;
  /* width: 336rpx; */
  font-size: 24rpx;
  color: #5c6171;
  line-height: 34rpx;
}

.container1 .photo {
  width: 278rpx;
  height: 370rpx;
}

.container1 .retakebox {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx 0;
}

.container1 .retakebox>image {
  width: 46rpx;
  height: 46rpx;
  margin-right: 18rpx;
}

.container1 .retakebox>text {
  text-decoration: underline;
  font-size: 28rpx;
  letter-spacing: 1px;
  line-height: 40rpx;
  color: #323541;
}

.container1 .graybtn {
  width: 638rpx;
  height: 94rpx;
  background: rgba(231, 231, 231, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 94rpx;
  color: #90919c;
  margin-top: 54rpx;
}

.container1 button::after {
  border: none;
}

.container1 button {
  margin-top: 40rpx;
  height: 94rpx;
  background: rgba(53, 62, 232, 1);
  border-radius: 0;
  color: #fff;
  font-size: 32rpx;
  line-height: 94rpx;
}

/* allready.wxss */

.container2 {
  padding: 80rpx 40rpx;
}

.container2 .bigtitle {
  font-size: 48rpx;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 66rpx;
  letter-spacing: 1px;
  margin-bottom: 18rpx;
}

.container2 .smalltitle {
  display: block;
  margin-top: 58rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
  line-height: 48rpx;
  letter-spacing: 2px;
}

.container2 .quernum {
  font-size: 28rpx;
  font-weight: 600;
  color: rgba(89, 96, 237, 1);
  line-height: 40rpx;
  letter-spacing: 1px;
  margin-top: 150rpx;
  text-align: center;
}

.container2 .count {
  width: 70rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  bottom: 260rpx;
  left: 341rpx;
}

.container2 .testbtn {
  width: 616rpx;
  height: 94rpx;
  background: rgba(231, 231, 231, 1);
  border-radius: 8rpx;
  line-height: 94rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
  text-align: center;
  position: absolute;
  bottom: 140rpx;
  left: 68rpx;
}

.container2 .btnactive {
  background: rgba(89, 96, 237, 1);
  color: #fff;
}

/* test/index.wxss */

.container {
  background-color: rgba(246, 247, 255, 1);
  /* padding-top: 16rpx; */
  box-sizing: border-box;
  height: 100vh;
}

.container .checkbluebox {
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  height: 40rpx;
  background: rgba(36, 88, 240, 1);
  border-radius: 6rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 40rpx;
  margin-right: 20rpx;
}

.container .timebox {
  width: 100%;
  height: 64rpx;
  background: rgba(232, 233, 243, 1);
  text-align: center;
  line-height: 64rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
}

.container .paper {
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  top: 32rpx;
  /* height: 1000rpx; */
  border-radius: 20rpx;
}
@media(min-height: 736px){
  .paper{
    height: 1228rpx !important;
  }
}
.container .notiphoneXpaper {
  bottom: 216rpx;
}
.container .isiphoneXpaper {
  bottom: 276rpx;
}

.container .paper swiper {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.container .paper swiper-item {
  height: 99% !important;
  overflow-y: scroll;
  border-radius: 40rpx;
  background-color: #fff;
  box-shadow: 0 6rpx 12rpx 0 rgba(0, 0, 0, 0.12);
}

.container scroll-view {
  height: 100%;
  /* background-color: #fff;
  box-shadow: 0 6rpx 12rpx 0 rgba(0, 0, 0, 0.12); */
}

.container .paperhead {
  padding: 20rpx 40rpx 50rpx 40rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: left;
  align-items: center;
  font-size: 30rpx !important;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  letter-spacing: 0.2rpx;
}

.container .title {
  font-size: 30rpx;
  font-weight: 600;
  line-height: 40rpx;
}

.container .title img {
  max-width: 100%;
}

/* paperbody */

.container .paperbody {
  background-color: #fff;
}

/* paper选项部分 */

.container .paper .options {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
  padding: 0rpx 30rpx 50rpx 30rpx;
  box-sizing: border-box;
  color: rgba(51, 51, 51, 1);
}

.container .lastbox {
  margin-top: -80rpx;
}

.container .lastoptions {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  color: rgba(51, 51, 51, 1);
  /* border: 1px solid red; */
}

.container .lasthead {
  margin-bottom: 20rpx;
  font-weight: 500;
}

.container .lasthead text {
  color: #fff;
  background-color: rgba(36, 88, 240, 1);
  border-radius: 5rpx;
  font-size: 22rpx;
  padding: 3rpx 15rpx;
  margin-left: 20rpx;
  font-weight: 400;
  letter-spacing: 2rpx;
}

.container .langoption {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-weight: 600;
  position: absolute;
  top: calc((100% - 30rpx)/2);
}

.container .optiontext {
  display: block;
  margin-left: 40rpx;
  height: 100%;
  line-height: 40rpx;
}

/* 选项未选中时样式 */

.container .selectItem {
  position: relative;
  display: flex;
  align-items: center;
  padding: 30rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  border-radius: 15rpx;
  color: #767676;
  background-color: rgba(250,250,250,1);
  font-size: 30rpx;
  font-weight: 600;
  font-family: PingFangSC-Semibold,PingFang SC;
}

/* 选中后的样式 */

.container .selectItem.active {
  background:rgba(53,62,232,0.1);
  box-shadow: 0px 4rpx 8rpx 0rpx rgba(0,0,0,0.15);
  border-radius: 20rpx;
  color:rgba(53,62,232,1);
}

/* 底部按钮 */

.container .bottombtn {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 152rpx;
  padding: 0 134rpx;
  background: transparent;
}

.container .isiphoneX {
  padding-bottom: 68rpx !important;
}

.container .btnprev, .container  .btnnext {
  font-size: 34rpx;
  font-weight: 600;
  line-height: 82rpx;
  height: 84rpx;
  text-align: center;
  color: rgba(36, 88, 240, 1);
  border-radius: 15rpx;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}
.btn-no-all-fill,.btn-is-all-fill{
  width: 180rpx !important;
  height: 70rpx;
  background-color: transparent;
  text-align: center;
  letter-spacing: 20rpx;
  text-indent: 20rpx;
  border-radius: 36rpx !important;
}
.btn-is-all-fill{
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.3);
}
.container  .btnnextUp {
  background-color: rgba(36, 88, 240, 1);
  color: #fff;
}

.container button::after {
  border: none;
}

.pageHide {
  opacity: 0;
  position: absolute;
  z-index: -1;
  height: 0;
  width: 0;
}

.chooseChapter {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.chooseChapter .paperTitle {
  padding: 40rpx;
  background: #fff;
  box-shadow: 0px 4rpx 14rpx 0px rgba(216, 219, 235, 1);
}

.chooseChapter .paperTitle .name {
  font-size: 48rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(50, 53, 65, 1);
  line-height: 66rpx;
}

.chooseChapter .paperTitle .mind {
  margin-top: 20rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(92, 97, 113, 1);
  line-height: 40rpx;
}

.chooseChapter .chapterList {
  position: absolute;
  top: 204rpx;
  bottom: 0;
  left: 0;
  right: 0;
  background: #f4f9fb;
  padding-top: 80rpx;
  overflow-y: auto;
}

.chooseChapter .chapterList .item {
  background: rgba(254, 255, 254, 1);
  box-shadow: 0px 4rpx 14rpx 0px rgba(216, 219, 235, 1);
  border-radius: 16rpx;
  margin: 0 40rpx 40rpx;
  padding: 18rpx 28rpx 20rpx;
}

.chooseChapter .chapterList .item .name {
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 50rpx;
  padding: 0 18rpx;
}

.chooseChapter .chapterList .item>.flex {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(50, 53, 65, 1);
  line-height: 40rpx;
  padding: 0 18rpx;
  margin-top: 30rpx;
}

.chooseChapter .chapterList .item .btn {
  margin-top: 70rpx;
  height: 94rpx;
  background: rgba(89, 96, 237, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 94rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.chooseChapter .chapterList .item .btn.btn1 {
  background: rgba(204, 206, 210, 1);
  border-radius: 8rpx;
}

.chooseChapter .chapterList .item image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 16rpx;
}

.get-user-phone-wrapper{
  display: flex;
  justify-content: space-between;
  height: 44rpx;
  margin-top: 40rpx;
  padding: 14rpx 0;
  border-bottom: 1px solid #e7e7e7;
}

.get-user-phone{
  display: inline-block;
  height: 100% !important;
  margin: 0 0 !important;
  line-height: 100% !important;
  color: rgba(53, 62, 232, 1) !important;
  background-color: white !important;
  padding: unset !important;
}
.multiple-question{
  background-color: rgba(250,250,250,1) !important;
}
.get-user-phone-wrapper > text{

}
.container .selectItem.notborderview,
.container .selectItem.active.notborderview{
  border:0px;
  color:#333;
  background-color: #fff !important;
  box-shadow: unset !important;
}
.container .selectItem.notborderview{
  flex-direction: column;
}
.container .selectItem.notborderview view{
  display: flex;
  justify-content: flex-start;
  width:100%;
  align-items: center;
}
.container .selectItem.notborderview view .langoption{
  position: relative;
  top:inherit;
  height:40rpx;
  line-height:40rpx;
}
.container .selectItem.notborderview view .optiontext{
  margin-left: 0;
}
.number_input{
  width:80rpx;
  height: 74rpx;
  border-radius:5px;
  border:1px solid rgba(217,217,217,1);
  text-align: center;
}
.number_slider_wrap{
  flex:1;
  display: flex;
  flex-direction: column;
}
.number_slider{
  width:calc(100% - 36px);
  z-index:10;
}
.container .selectItem.notborderview view  .number_slider_tip{
  width:calc(100% - 36px);
  display: flex;
  justify-content: space-between;
}
.number_slider_tip_item_1{
  width:20rpx;
}
.number_slider_tip_item_center{
  display: flex;
  justify-content: space-between;
  flex:1;
}
.number_slider_tip_number_label{
  position:relative;
}
.number_slider_tip_number_label:before{
  position: absolute;
  content:"|";
  color:rgba(153, 153, 153, 1);
  transform:translate(50%,-90%);
}
.question-number-group{
  display: flex;
  align-items: center;
  padding: 28rpx 0 20rpx 40rpx;
  color: #2F3038;
  font-family: PingFangSC-Semibold,PingFang SC;
}
.question-number-group .current-question{
  display: flex;
  flex-direction: column;
  font-size: 56rpx;
  font-weight: 600;
  color: rgba(47,48,56,1);
  line-height: 80rpx;
}
.underline-ques{
  display: inline-block;
  width: 68rpx;
  height: 8rpx;
  background:rgba(53,62,232,1);
  border-radius: 4rpx;
}
.question-number-group .total-question,.fragment-question{
  height: 42rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(197,197,197,1);
  line-height: 42rpx;
}
.fragment-question{
  margin-left: 10rpx;
}
